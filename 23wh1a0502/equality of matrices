#include<stdio.h>
int main(){
int mat1[2][2],mat2[2][2],i,j;
printf("enter 4 numbers");
for(i=0;i<4;i++){
  for(j=0;j<4;j++){
  scanf("%d",mat1[i][j]);
}
for(i=0;i<4;i++){
  for(j=0;j<4;j++){
  scanf("%d",&mat2[i][j]);
  }
int count=0;
 for(i=0;i<4;i++){
 for(j=0;j<4;j++){
   if(mat1[i][j]==mat2[i][j]);
       count++;
       }
       }
if(count==4){
  printf("matrices are equal");
else{
  printf("matrices are not equal");
  }
  }
  }
  }
  }
